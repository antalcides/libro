\end_layout

\begin_layout Plain Layout


\backslash
node[scale=.8] (a) at (0,0){
\end_layout

\begin_layout Plain Layout

  
\backslash
begin{tikzcd}%[row sep=2cm,column sep=2cm,inner sep=1ex]  
\end_layout

\begin_layout Plain Layout

   A 
\backslash
arrow[dash,thick,green]{d} & & B 
\backslash
arrow[dash,thick,green]{d} 
\backslash

\backslash

\end_layout

\begin_layout Plain Layout

    A-B 
\backslash
arrow[dash,thick,blue]{dr} & & B-A 
\backslash
arrow[dash,thick,blue]{dl}
\backslash

\backslash
 
\end_layout

\begin_layout Plain Layout

    & 
\backslash
emptyset & 
\backslash

\backslash

\end_layout

