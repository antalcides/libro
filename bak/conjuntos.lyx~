draw[thick] 
\backslash
rectangulo node [below left of=c2, yshift=2.3cm, xshift=2.7cm]  {$U$};
\end_layout

\begin_layout Plain Layout

         
\backslash
end{tikzpicture}   
\end_layout

\begin_layout Plain Layout

       
\backslash
subcaption{$A
\backslash
nsubseteq B$}
\end_layout

\begin_layout Plain Layout

    
