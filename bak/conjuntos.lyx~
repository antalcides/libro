centering
\end_layout

\begin_layout Plain Layout

               
\backslash
begin{tikzpicture}[scale=0.5,every node/.style={scale=0.5}]               
     
\backslash
fill[gray!30,thick] 
\backslash
rectangulo;
\end_layout

\begin_layout Plain Layout

          
\backslash
fill[red!30,thick] 
\backslash
secondcircle; 
\end_layout

\begin_layout Plain Layout

         
\backslash
fill[blue!30,thick] 
\backslash
firstcircle;
\end_layout

\begin_layout Plain Layout

    
\backslash
draw[thick]
\backslash
firstcircle node[below](c1) {$A$}; 
\end_layout

\begin_layout Plain Layout

   
\backslash
draw[thick] 
\backslash
secondcircle node [above](c2) {$B$};
\end_layout

\begin_layout Plain Layout

       
