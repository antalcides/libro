\begin_layout Standard
\begin_inset CommandInset label
LatexCommand label
name "sec:problema2"

\end_inset

 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
problemas
\end_layout

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout

{
\end_layout

\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{enumerate} 
\end_layout

\begin_layout Plain Layout


\backslash
item Demuestra que si $A$ es un conjunto, la colección de todos los objetos
 que no son elementos de $A$ no es un conjunto.
 
\end_layout

\begin_layout Plain Layout


\backslash
item Demuestre que $A
\backslash
cap B=A
\backslash
dif (A
\backslash
dif B)$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item Si $A
\backslash
cup B
\backslash
subset C$, entonces $A
\backslash
dif B=A
\backslash
cap (C
\backslash
dif B)$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
subset B$ si y sólo si para todo conjunto $E$, $E
\backslash
dif B
\backslash
subset E
\backslash
dif A$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
dif (B
\backslash
dif C)=(A
\backslash
dif B)
\backslash
cup (A
\backslash
cap C)$.
\end_layout

\begin_layout Plain Layout


\backslash
item Muestra por medio de ejemplos que las siguientes proposiciones son
 falsas:  
\backslash
begin{enumerate}   
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
dif B=B
\backslash
dif A$.
   
\end_layout

\begin_layout Plain Layout


\backslash
item Si $A
\backslash
subset B
\backslash
cup C$ entonces $A
\backslash
subset B$ o $A
\backslash
subset C$.
 
\end_layout

\begin_layout Plain Layout


\backslash
end{enumerate} 
\end_layout

\begin_layout Plain Layout


\backslash
newcommand{
\backslash
tri}{
\backslash
triangle} 
\end_layout

\begin_layout Plain Layout


\backslash
item Definimos la 
\backslash
emph{diferencia simétrica} de $A$ y $B$ como el conjunto $(A
\backslash
dif B)
\backslash
cup (B
\backslash
dif A)$ y lo denotamos por $A
\backslash
tri B$.
 Demuestra que:  
\end_layout

\begin_layout Plain Layout


\backslash
begin{enumerate}    
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
tri 
\backslash
emptyset=A$.
    
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
tri B=
\backslash
emptyset$ si y sólo si $A=B$.
  
\end_layout

\begin_layout Plain Layout

 
\backslash
item Si $A
\backslash
tri B= A
\backslash
tri C$, entonces $B=C$.
  
\end_layout

\begin_layout Plain Layout


\backslash
end{enumerate} 
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
times B=
\backslash
emptyset$ si y sólo si $A=
\backslash
emptyset$ o $B=
\backslash
emptyset$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
times B=B
\backslash
times A$ si y sólo si $A=B$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
times (B
\backslash
cup C)=(A
\backslash
times B)
\backslash
cup (B
\backslash
times C)$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item $A
\backslash
times (B
\backslash
cap C)=(A
\backslash
times B)
\backslash
cap (B
\backslash
times C)$.
 
\end_layout

\begin_layout Plain Layout


\backslash
item Usando las leyes de De Morgan  demuestra 
\end_layout

\begin_layout Plain Layout

   $A
\backslash
cup(B
\backslash
cup C)=(A
\backslash
cup B)
\backslash
cup C$.
    
\end_layout

\begin_layout Plain Layout


\backslash
item $
\backslash
po{A} 
\backslash
cap 
\backslash
po{B}=
\backslash
po{A
\backslash
cap B}$ 
\end_layout

\begin_layout Plain Layout


\backslash
item 
\backslash
label{itm:first} $
\backslash
po{A} 
\backslash
cup 
\backslash
po{B}
\backslash
subset
\backslash
po{A
\backslash
cup B}$
\backslash
  
\end_layout

\begin_layout Plain Layout


\backslash
item Muestra con un ejemplo que la igualdad en el ejercicio 
\backslash
ref{itm:first} no siempre se cumple.
  
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Determinar por extensión los siguientes conjuntos
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A=\left\{ x\in I\!N\,:\,x\leq3\vee5<x<7\right\} $
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $B=\left\{ y\in\mathcal{\mathfrak{\mathcal{\mathbb{Z}}}}\,:\,y=x^{2}-1\wedge-1<x<3,\,x\in\mathbb{Z}\right\} $
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
resizebox{0.9
\backslash
hsize}{!}{
\end_layout

\end_inset


\begin_inset Formula $C=\left\{ y\in\mathrm{\mathbb{Z\,\mathrm{:}\,\mathrm{y}=\mathrm{3-5x\wedge-2<x<5\wedge3\leq x\leq8,}\,\mathrm{x}\in Z}}\right\} $
\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout

}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Determinar por compresión el conjunto 
\begin_inset Formula $T=\left\{ -1,1,2\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Si 
\begin_inset Formula $A=\left\{ 2,3,5,7\right\} $
\end_inset

, indica cuales de las siguientes afirmaciones son verdaderas y cuales son
 falsas
\end_layout

\begin_layout Enumerate
\begin_inset Formula $5\in A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $3\subset A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left\{ 7\right\} \subset A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left\{ 3,5\right\} \in A.$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Si 
\begin_inset Formula $A=\left\{ x\in I\!N\,:\,x\leq2\vee x=7\right\} ,$
\end_inset

 hallar todos los subconjuntos propios de 
\begin_inset Formula $A.$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Dados los siguientes conjuntos
\end_layout

\begin_layout Standard
\begin_inset Formula $A=\left\{ y\in\mathbb{Z}\::\,y=7x+2\wedge x\in\mathbb{Z}\right\} ,\,$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $B=\left\{ y\in\mathbb{Z}\,:\:y=7x-26\wedge x\in\mathbb{Z}\right\} ,\,$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $C=\left\{ y\in\mathbb{Z}\text{\,:\,\ \ensuremath{y}\ensuremath{=4x+1\wedge x\in\mathbb{Z}}}\right\} $
\end_inset

 y 
\end_layout

\begin_layout Standard
\begin_inset Formula $D=\left\{ y\in\mathbb{Z}\,:\:y=2x+1\wedge x\in\mathbb{Z}\right\} ,$
\end_inset

 analizar y justificar debidamente su conclusión en cada caso:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A=B$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $C=D$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $C\subset D$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\cup B$
\end_inset

=
\begin_inset Formula $A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $C\cap D=\left\{ \right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Sean 
\begin_inset Formula $U=\left\{ 1,2,3,4,5,6,7,8,9,0\right\} ,\,A=\left\{ 2,4,6,8\right\} $
\end_inset

 y 
\begin_inset Formula $C=\left\{ 3,4,5\right\} .$
\end_inset

 Al hallar un subconjunto 
\begin_inset Formula $X$
\end_inset

 de 
\begin_inset Formula $U$
\end_inset

 tal que 
\begin_inset Formula $X\subset C,\,x\nsubseteq A,\,X\nsubseteq B,$
\end_inset

 cuántas soluciones existen.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

Cuántos de los siguientes conjuntos son vacíos:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A=\left\{ x\in U\,:\:x\notin U\right\} $
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $B=\left\{ x\in\mathbb{Z}\,:\:y=x^{3}=3,\,y\in\mathbb{Z}\right\} $
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $C=\left\{ y\in I\!R\,:\,y=\frac{1}{x},x\in\mathbb{Z}\right\} $
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $D=\left\{ x\in\mathbb{Q}\,:\,x^{2}-1=2\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

Si 
\begin_inset Formula $A,\,B$
\end_inset

 y 
\begin_inset Formula $C$
\end_inset

 son conjuntos tal que 
\begin_inset Formula $A\subseteq B\subseteq C$
\end_inset

 ¿Cuál es la relación entre 
\begin_inset Formula $C-B$
\end_inset

 y 
\begin_inset Formula $C-A$
\end_inset

?
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Si 
\begin_inset Formula $A,\,B,\,C$
\end_inset

 y 
\begin_inset Formula $D$
\end_inset

 son conjuntos.
 Demostrar las siguientes propiedades
\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq U,\:A\cup U=U$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $B\subseteq A\cup B$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq C\wedge B\subseteq C\Rightarrow A\cup B\subseteq C$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\cap B\subseteq B$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq C$
\end_inset

 y 
\begin_inset Formula $B\subseteq D\Rightarrow\mbox{\ensuremath{A\cap B\subseteq C\cap D}}$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq U$
\end_inset

, 
\begin_inset Formula $A\cap U=A$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq B\Rightarrow A\cap C\subseteq B\subseteq C,\forall C$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A-A=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\emptyset-A=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\cap\left(B-C\right)=\left(A\cap B\right)-\left(A\cap C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A-\emptyset=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A-B\neq B-A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A-B\right)\subseteq A$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq B\Rightarrow A-C\subseteq B-C,\,\forall C$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq B\Rightarrow A-B=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $B\cap\left(A-B\right)=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A$
\end_inset

 y 
\begin_inset Formula $B$
\end_inset

 son disjuntos, entonces 
\begin_inset Formula $A\cap B=\emptyset$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Si 
\begin_inset Formula $A,\,B$
\end_inset

 y 
\begin_inset Formula $C$
\end_inset

 son conjuntos y 
\begin_inset Formula $U$
\end_inset

 el conjunto definido en 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
ref{uni}
\end_layout

\end_inset

 demostrar las siguientes propiedades
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A^{c}\right)^{c}=A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\cap A^{c}=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq U,$
\end_inset

 entonces 
\begin_inset Formula $A\cup A_{U}^{c}=U$
\end_inset


\end_layout

\begin_layout Enumerate
Si 
\begin_inset Formula $A\subseteq B,$
\end_inset

 entonces 
\begin_inset Formula $B^{c}\subseteq A^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\cup B\right)^{c}=A^{c}\cap B^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\cap B\right)^{c}=A^{c}\cup B^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\triangle A=\emptyset$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\triangle\emptyset=A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\triangle B=B\triangle A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\triangle B\right)\triangle C=A\triangle\left(B\triangle C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\triangle B\right)\cap C=\left(A\cap C\right)\triangle\left(B\cap C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\triangle B\right)\cup\left(B\triangle C\right)=\left(A\cup B\cup C\right)-\left(A\cap B\cap C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\emptyset_{r}^{c}=U$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $U^{c}=\emptyset_{r}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Dados los conjuntos 
\begin_inset Formula $A=\left\{ a,c,d\right\} ,\,B=\left\{ e,f,g\right\} $
\end_inset

 y 
\begin_inset Formula $C=\left\{ l,e,j,k\right\} ,$
\end_inset

 hallar 
\begin_inset Formula $A\cup\left(B\cap C\right).$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

Si 
\begin_inset Formula $U=\left\{ a,b,c,d,e\right\} ,$
\end_inset

 
\begin_inset Formula $A\cup B=\{a,c\}$
\end_inset

 y 
\begin_inset Formula $A-B=\left\{ b\right\} .$
\end_inset

 Hallar 
\begin_inset Formula $A$
\end_inset

 y 
\begin_inset Formula $B.$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

 Considere los conjuntos 
\begin_inset Formula $A=\left\{ x\in I\!N\,:\,x\right\} \mbox{es divisor de \ensuremath{12}}$
\end_inset

, 
\begin_inset Formula $B=\left\{ x\in I\!N\,:\,x\mbox{ es divisor de \ensuremath{18}}\right\} $
\end_inset

 y 
\begin_inset Formula $C=\left\{ x\in I\!N\,:\,x\mbox{ es divisor de \ensuremath{16}}\right\} .$
\end_inset

 Hallar 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A-B\right)\cap\left(B-C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A-B\right)\cup\left(B-C\right).$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

Dados los conjuntos 
\begin_inset Formula $A=\left\{ 1,2,5,7,8\right\} ,$
\end_inset

 
\begin_inset Formula $B=\left\{ 2,3,4,7,9\right\} ,$
\end_inset

 
\begin_inset Formula $C=\left\{ 1,3,5,6,8\right\} $
\end_inset

 y 
\begin_inset Formula $U=\left\{ x\in I\!N\,:\,x<10\right\} .$
\end_inset

 Hallar
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left[\left(A\cup B\right)-\left(A\cap C\right)\right]^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left[\left(A\cap B\right)-\left(A\cup B\right)\right]^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left[\left(A-B\right)\cup\left(A-C\right)\right]^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left[\left(A^{c}-B\right)\cap\left(A-C\right)\right]^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left[\left(C-B^{c}\right)-\left(A^{c}\cup c\right)\right]^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A^{c}-B^{c}\right)\triangle\left(B^{c}\cup C\right)^{c}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
item 
\end_layout

\end_inset

Utilice el diagrama de Venn-Euler para demostrar las siguientes propiedades
\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\cup B=B\cup A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\cap B=B\cap A$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\cap B\right)\cap C=A\cap\left(B\cap C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(A\cup B\right)\cup C=A\cup\left(B\cup C\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A^{c}\cap B^{c}=\left(A\cup B\right)^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A^{c}\cup B^{c}=\left(A\cup B\right)^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A-B=A\cap B^{c}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $A\subseteq A\cup B.$
\end_inset


\end_layout

