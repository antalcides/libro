\begin_layout Subsubsection
Intersección de conjuntos
\end_layout

\begin_layout Standard
Sean 
\begin_inset Formula $A,B\subseteq U$
\end_inset

 el conjunto 
\begin_inset Formula $A\text{\cap\ensuremath{B}}$
\end_inset

 se representa si 
\begin_inset Formula $A\cap B\neq\emptyset$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{figure}[H]
\end_layout

\begin_layout Plain Layout


\backslash
centering
\end_layout

\begin_layout Plain Layout


