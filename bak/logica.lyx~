\begin_layout Plain Layout

}
\end_layout

\end_inset

Complemento directo: 
\end_layout

\begin_layout Enumerate
Veo
\family sans
 lo que dices
\family default
.
 
\end_layout

\begin_layout Enumerate
Pide 
\family sans
lo que quieras
\family default
.
\end_layout

\begin_layout Enumerate
Olvidé decirle a Carlos 
\family sans
lo que me habías encargado
\family default
.
 
\end_layout

\begin_layout Enumerate
Dijeron 
\family sans
que vendrían hoy
\family default
.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{ejems}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
