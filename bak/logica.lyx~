\begin_layout Enumerate
Este queda de ejercicio al lector.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{ejems}{Falacias}
\end_layout

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $p\wedge\sim p$
\end_inset

 (principio de contradicción) 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $(p\rightarrow q)\wedge\left(p\wedge\sim q\right)$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\sim\left(p\vee\sim p\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{ejems}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
solucion
\end_layout

\end_inset


\end_layout

