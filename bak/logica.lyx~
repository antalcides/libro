\begin_inset Caption Standard

\begin_layout Plain Layout
Cometa
\end_layout

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
label{com}
\end_layout

\end_inset


\begin_inset ERT
status open

